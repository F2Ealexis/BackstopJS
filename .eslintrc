{
  "env": {
    "node": true
  },
  "globals": {},
  "rules": {
    "camelcase": 0,
    "comma-style": [
      2,
      "last"
    ],
    "curly": 2,
    "eqeqeq": 2,
    "no-extend-native": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "new-cap": 0,
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ],
    "max-depth": [
      2,
      3
    ],
    "max-statements": [
      2,
      50
    ],
    "no-eq-null": 2,
    "block-scoped-var": 2,
    "no-undef": 2,
    "no-unused-vars": 0,
    "no-empty": 2
  }
}
